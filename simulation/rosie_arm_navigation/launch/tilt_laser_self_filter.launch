<launch>
   <node machine="c2"  pkg="robot_self_filter" type="self_filter" name="pr2_arm_navigation_tilt_laser_self_filter" output="screen">
     <!-- I'm sorry for the above. -->

     <remap from="robot_description" to="robot_description" />
     
     <!-- The topic for the input cloud -->
     <remap from="cloud_in" to="tilt_scan_cloud_known2" />
     
     <!-- The topic for the output cloud -->
     <remap from="cloud_out" to="tilt_scan_cloud_filtered2" />

     <!-- The frame of the sensor used to obtain the data to be
       filtered; This parameter is optional. If it is not specified,
       shadow points will be considered outside -->
     <param name="sensor_frame" type="string" value="shoulder_tilting_laser" />

     <param name="subsample_value" type="double" value=".01"/>

     <rosparam command="load" file="$(find rosie_arm_navigation)/config/self_filter.yaml" />

   </node>
</launch>
