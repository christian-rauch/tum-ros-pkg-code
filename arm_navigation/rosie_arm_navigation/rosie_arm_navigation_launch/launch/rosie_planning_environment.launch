<launch> 
        <!-- send parameters for collision checking for Rosie; this includes parameters for the self filter -->
        <rosparam command="load" ns="robot_description_collision" file="$(find rosie_arm_navigation)/config/collision_checks_both_arms.yaml" />

        <!-- send parameters for multidof -->
        <rosparam command="load" ns="robot_description_planning" file="$(find rosie_arm_navigation)/config/multi_dof_joints.yaml" />

        <!-- send parameters needed for motion planning -->
        <rosparam command="load" ns="robot_description_planning" file="$(find rosie_arm_navigation)/config/planning_groups.yaml" />

</launch>
