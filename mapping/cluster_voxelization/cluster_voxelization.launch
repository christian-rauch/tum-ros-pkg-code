<launch>

  <node pkg="cluster_voxelization" type="cluster_voxelization_node" name="cluster_voxelization" respawn="true" output="screen">
    <param name="~table_min_height"                 value="0.5" />
    <param name="~table_max_height"                 value="1.5" />
    <param name="~table_clusters_growing_tolerance" value ="0.1" />

    <param name="~downsample_leaf_width_x" value="0.03" />
    <param name="~downsample_leaf_width_y" value="0.03" />
    <param name="~downsample_leaf_width_z" value="0.03" />
    <param name="~search_k_closest"        value="10" />

    <param name="~normal_eps_angle" value="15.0" />

    <param name="~region_angle_threshold" value="30.0" />

    <param name="~clusters_growing_tolerance" value="0.5" />
    <param name="~clusters_min_pts"           value="10" />

    <param name="~object_cluster_tolerance" value="0.05" />
    <param name="~object_cluster_min_pts"   value="30" />

    <param name="~table_delta_z"    value="0.03" />
    <param name="~object_min_distance_from_table"  value="0.10" />
  </node>
</launch>
