<launch>
  <node pkg="player_log_actarray" type="pointcloud_delimiter" name="pointcloud_delimiter" output="screen">
    <param name="~dir" type="string"  value="" />
    <param name="~max_x" value="1.75"/>
    <param name="~min_x" value="1.45"/>
    <param name="~max_y" value="0.1"/>
    <param name="~min_y" value="-0.3"/>
    <param name="~max_z" value="0.05"/>
    <param name="~min_z" value="-0.35"/>
    <!--set if you want to normalize points (such as from david system) -->
    <param name="~normalize" value="0"/>		
    <param name="~norm" value="1000.0"/>
  </node>
</launch>
